        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp 	"
            android:background="@color/black" />
    </LinearLayout>

</ScrollView>