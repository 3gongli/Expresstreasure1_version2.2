<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#efefef"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
        <RelativeLayout 
	        android:id="@+id/relativelayout_title"
	        android:layout_width="match_parent"
	        android:layout_height="50dp"
	        android:background="#017eff">
	        <ImageView 
	            android:id="@+id/imageView_back_login"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:src="@drawable/back_login"
	            android:clickable="true"
	            android:layout_centerVertical="true"
	            android:layout_marginLeft="20dp"/>
	        <TextView 
	            android:id="@+id/textView_title"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="完成订单"
	            android:layout_centerInParent="true"
	            android:textColor="@color/white"
	            android:textSize="19sp"/>
	    </RelativeLayout>
	    <RelativeLayout 
	        android:id="@+id/relativeLayout_order_id"
	        android:layout_width="match_parent"
	        android:layout_height="45dp"
	        android:layout_marginTop="8dp"
	        android:background="@color/white">
	        <TextView 
	            android:id="@+id/textView1"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="订单编号:"
	            android:layout_marginLeft="18dp"
	            android:textColor="#1a1a1a"
	            android:textSize="16sp"
	            android:layout_centerVertical="true"/>
	        <TextView 
	            android:id="@+id/textView_order_id"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_toRightOf="@+id/textView1"
	            android:text="1234453453"
	            android:layout_marginLeft="5dp"
	            android:textColor="#606060"
	            android:textSize="16sp"
	            android:layout_centerVertical="true"/>
	    </RelativeLayout>
	    <RelativeLayout 
	        android:id="@+id/relativeLayout_shipper"
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:layout_marginTop="8dp"
	        android:background="@color/white">
	        <ImageView 
	            android:id="@+id/imageView_shipper_info"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="18dp"
	            android:layout_centerVertical="true"
	            android:src="@drawable/shop"/>
	        <TextView 
	            android:id="@+id/textView2"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="商家信息"
	            android:layout_marginLeft="5dp"
	            android:layout_toRightOf="@+id/imageView_shipper_info"
	            android:textColor="#1a1a1a"
	            android:textSize="16sp"
	            android:layout_centerVertical="true"/>
	    </RelativeLayout>
	    <TextView 
	        android:layout_width="match_parent"
	        android:layout_height="1dp"
	        android:background="#efefef"/>

	    <RelativeLayout
	        android:id="@+id/relativeLayout_shipper_info"
	        android:layout_width="match_parent"
	        android:layout_height="60dp"
	        android:background="@color/white" >
	
	        <TextView
	            android:id="@+id/textView_shipper_name"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="10dp"
	            android:layout_marginLeft="18dp"
	            android:text="兰州拉面"
	            android:textSize="16sp"
	            android:textColor="#1a1a1a" />
	        <ImageView 
	            android:id="@+id/imageView_shipper_name"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_toRightOf="@+id/textView_shipper_name"
	            android:layout_marginLeft="5dp"
	            android:layout_marginTop="10dp"
	            android:src="@drawable/shoplogo"/>
	        <TextView 
	            android:id="@+id/textView_shipper_phone"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentRight="true"
	            android:layout_marginRight="18dp"
	            android:text="010-11223311"
	            android:layout_marginTop="10dp"
	            android:textColor="#1a1a1a"/>
	        <TextView 
	            android:id="@+id/textView_shipper_address"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="18dp"
	            android:layout_below="@+id/textView_shipper_name"
	            android:layout_marginTop="3dp"
	            android:textColor="#606060"
	            android:text="左岸公社706"/>
	    </RelativeLayout>
	    
	    <TextView 
	        android:layout_width="match_parent"
	        android:layout_height="1dp"
	        android:background="#efefef"/>
	    
	    <RelativeLayout 
	        android:id="@+id/relativeLayout_order"
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:layout_marginTop="8dp"
	        android:background="@color/white">
	        <ImageView 
	            android:id="@+id/imageView_order"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="18dp"
	            android:layout_centerVertical="true"
	            android:src="@drawable/reminders"/>
	        <TextView 
	            android:id="@+id/textView4"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="订单详情"
	            android:textSize="16sp"
	            android:layout_marginLeft="5dp"
	            android:layout_toRightOf="@+id/imageView_order"
	            android:textColor="#1a1a1a"
	            android:layout_centerVertical="true"/>
    	</RelativeLayout>
	    
	    <RelativeLayout 
	        android:id="@+id/relativeLayout_order_detail"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:background="@color/white">
	        <TextView 
	            android:id="@+id/textView7"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="18dp"
	            android:layout_marginTop="5dp"
	            android:textColor="#1a1a1a"
	            
	            android:text="订单类型:"/>
	        <TextView 
	            android:id="@+id/textView_goods_type"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="5dp"
	            android:layout_marginLeft="5dp"
	            android:layout_toRightOf="@+id/textView7"
	            android:layout_below="@+id/textView5"
	            android:textColor="#1a1a1a"
	            android:text="餐饮"/>
	        <TextView 
	            android:id="@+id/textView8"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="18dp"
	            android:layout_marginTop="5dp"
	            android:textColor="#1a1a1a"
	            android:layout_below="@+id/textView7"
	            
	            android:text="商家留言:"/>
	        <TextView 
	            android:id="@+id/textView_shipper_remarks"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="5dp"
	            android:layout_marginLeft="5dp"
	            android:layout_toRightOf="@+id/textView8"
	            android:layout_below="@+id/textView7"
	            android:textColor="#1a1a1a"
	            android:text=""/>
	        <TextView 
	            android:id="@+id/textView9"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_below="@+id/textView8"/>
	    </RelativeLayout>
	    
	    <RelativeLayout 
	        android:id="@+id/relativeLayout_info_complete"
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:layout_marginTop="8dp"
	        android:background="@color/white">
	        <ImageView 
	            android:id="@+id/imageView_info_complete"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="18dp"
	            android:layout_centerVertical="true"
	            android:src="@drawable/pencil"/>
	        <TextView 
	            android:id="@+id/textView10"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="信息补录"
	            android:textSize="16sp"
	            android:layout_marginLeft="5dp"
	            android:layout_toRightOf="@+id/imageView_info_complete"
	            android:textColor="#1a1a1a"
	            android:layout_centerVertical="true"/>
	        
	    </RelativeLayout>
	    <RelativeLayout 
            android:id="@+id/relativeLayout_camera1"
            android:layout_width="match_parent"
            android:background="@color/white"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/textView_goods_address" 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="18dp"
                android:focusable="true"
        		android:focusableInTouchMode="true"
                android:textColor="#1a1a1a"
                android:text="收货地址:"/>
            <EditText 
                android:id="@+id/editText_goods_address"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_toRightOf="@+id/textView_goods_address"
                android:layout_marginTop="7dp"
                android:background="@drawable/lalal"
                android:hint="请输入小票上的收货地址"
                android:text="左岸公社"
                android:singleLine="false"
                android:textSize="13sp"
                android:layout_marginLeft="15dp"/>
            <TextView
                android:id="@+id/fenge1" 
                android:layout_below="@+id/textView_goods_address"
		        android:layout_width="match_parent"
		        android:layout_marginTop="5dp"
		        android:layout_height="1dp"
		        android:background="#efefef"/>
            <TextView 
                android:id="@+id/textView_goods_name"
                android:layout_below="@+id/fenge1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="18dp"
                android:textColor="#1a1a1a"
                android:text="联  系  人:"/>
            <EditText 
                android:id="@+id/editText_goods_name"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_toRightOf="@+id/textView_goods_name"
                android:layout_below="@+id/fenge1"
                android:layout_marginTop="7dp"
                android:background="@drawable/lalal"
                android:hint="请输入联系人姓名"
                android:text="宋"
                android:textSize="13sp"
                android:layout_marginLeft="15dp"/>
            <TextView
                android:id="@+id/fenge2" 
                android:layout_below="@+id/textView_goods_name"
		        android:layout_width="match_parent"
		        android:layout_marginTop="5dp"
		        android:layout_height="1dp"
		        android:background="#efefef"/>
            <TextView 
                android:id="@+id/textView_goods_phone"
                android:layout_below="@+id/fenge2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="18dp"
                android:textColor="#1a1a1a"
                android:text="联系电话:"/>
            <EditText 
                android:id="@+id/editText_goods_phone"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_toRightOf="@+id/textView_goods_phone"
                android:layout_below="@+id/fenge2"
                android:layout_marginTop="7dp"
                android:background="@drawable/lalal"
                android:hint="请输入联系人电话"
                android:text="12345678"
                android:textSize="13sp"
                android:layout_marginLeft="15dp"/>
            <TextView
                android:id="@+id/fenge3" 
                android:layout_below="@+id/textView_goods_phone"
		        android:layout_width="match_parent"
		        android:layout_marginTop="5dp"
		        android:layout_height="1dp"
		        android:background="#efefef"/>
        </RelativeLayout>
        <Button 
	        android:id="@+id/button_confirm"
	        android:layout_width="match_parent"
	        android:layout_height="45dp"
	        android:layout_marginTop="10dp"
	        android:layout_marginLeft="18dp"
	       	android:layout_marginRight="18dp"
	        android:background="@drawable/selector_button_login"
            android:textSize="19sp"
	        android:text="确  定"
	        android:textColor="@color/white"/>
    </LinearLayout>
    

</ScrollView>
